e=this.evaluateConst(c);if(null==e)throw Error(`Expected a constant${module$asm$token.Token.at(c)}`);let f;if(d<a.length){if(!module$asm$token.Token.eq(a[d],module$asm$token.Token.COMMA))throw Error(`Expected comma: ${module$asm$token.Token.nameAt(a[d])}`);f=module$asm$token.Token.expectIdentifier(a[d+1]);module$asm$token.Token.expectEol(a[d+2])}const g=[];let h=1;for(;0<h;)a=null!==(b=this.stream.next())&&void 0!==b?b:fail$$module$asm$preprocessor(".repeat with no .endrep"),module$asm$token.Token.eq(a[0],
module$asm$token.Token.REPEAT)&&h++,module$asm$token.Token.eq(a[0],module$asm$token.Token.ENDREPEAT)&&h--,module$asm$token.Token.eq(a[0],module$asm$token.Token.ENDREP)&&h--,g.push(a);this.repeats.push([g,e,-1,f]);this.parseEndRepeat(a)}parseEndRepeat(a){module$asm$token.Token.expectEol(a[1]);const b=this.repeats.pop();if(!b)throw Error(`.endrep with no .repeat${module$asm$token.Token.at(a[0])}`);++b[2]>=b[1]||(this.repeats.push(b),this.stream.unshift(...b[0].map((a)=>a.map((a)=>{if("ident"!==a.token||
a.str!==b[3])return a;const c={token:"num",num:b[2]};a.source&&(c.source=a.source);return c}))))}parseIf(a){let b=1,c=!1;const d=[];for(;0<b;){const e=this.stream.next();if(!e)throw Error("EOF looking for .endif");const f=e[0];if(module$asm$token.Token.eq(f,module$asm$token.Token.ENDIF)){if(b--,!b)break}else if("cs"===f.token&&f.str.startsWith(".if"))b++;else if(1===b&&!c)if(a&&(module$asm$token.Token.eq(f,module$asm$token.Token.ELSE)||module$asm$token.Token.eq(f,module$asm$token.Token.ELSEIF))){a=
!1;c=!0;continue}else if(module$asm$token.Token.eq(f,module$asm$token.Token.ELSEIF)){a=!!this.evaluateConst(parseOneExpr$$module$asm$preprocessor(e.slice(1),f));continue}else if(module$asm$token.Token.eq(f,module$asm$token.Token.ELSE)){a=!0;continue}a&&d.push(e)}this.stream.unshift(...d)}}function parseOneIdent$$module$asm$preprocessor(a,b){a=parseOneExpr$$module$asm$preprocessor(a,b);return module$asm$expr.Expr.identifier(a)}
function parseOneExpr$$module$asm$preprocessor(a,b){if(!a.length){if(!b)throw Error("Expected expression");throw Error(`Expected expression: ${module$asm$token.Token.nameAt(b)}`);}return module$asm$expr.Expr.parseOnly(a)}function noGarbage$$module$asm$preprocessor(a){if(a)throw Error(`garbage at end of line: ${module$asm$token.Token.nameAt(a)}`);}function badClose$$module$asm$preprocessor(a,b){throw Error(`${module$asm$token.Token.name(b)} with no ${a}${module$asm$token.Token.at(b)}`);}
function fail$$module$asm$preprocessor(a){throw Error(a);}var module$asm$preprocessor={};module$asm$preprocessor.Preprocessor=Preprocessor$$module$asm$preprocessor;const MAX_DEPTH$$module$asm$tokenstream=100;
class TokenStream$$module$asm$tokenstream{constructor(){this.stack=[]}next(){for(;this.stack.length;){const [a,b]=this.stack[this.stack.length-1];if(b.length)return b.pop();const c=null===a||void 0===a?void 0:a.next();if(c)return c;this.stack.pop()}}unshift(...a){if(!this.stack.length)throw Error("Cannot unshift after EOF");const b=this.stack[this.stack.length-1][1];for(let c=a.length-1;0<=c;c--)b.push(a[c])}enter(a){const b=[void 0,[]];a&&(b[0]=a);this.stack.push(b);if(this.stack.length>MAX_DEPTH$$module$asm$tokenstream)throw Error("Stack overflow");
}exit(){this.stack.pop()}}var module$asm$tokenstream={};module$asm$tokenstream.TokenStream=TokenStream$$module$asm$tokenstream;const OPTIONAL$$module$flagset=(a)=>"",NO_BANG$$module$flagset=(a)=>!0===a?!1:a;class Flag$$module$flagset{constructor(a,b){this.flag=a;this.opts=b;Flag$$module$flagset.flags.set(a,this)}static all(){return[...this.flags.values()]}}Flag$$module$flagset.flags=new Map;
class Preset$$module$flagset{constructor(a,b,c,d){this.name=b;this.description=c;this.flags=d.map((a)=>a instanceof Flag$$module$flagset?[a,!0]:a);a.presets.set(mapPresetName$$module$flagset(b),this)}static all(){Presets$$module$flagset.instance||(Presets$$module$flagset.instance=new Presets$$module$flagset);return[...Presets$$module$flagset.instance.presets.values()]}get flagString(){null==this._flagString&&(this._flagString=String(new FlagSet$$module$flagset(`@${this.name}`)));return this._flagString}}
